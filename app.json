{
  "expo": {
    "name": "Just Sell It",
    "slug": "JustSellIt",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/appiconwithtitle.png",
    "splash": {
      "image": "./src/assets/splashblue.png",
      "resizeMode": "contain",
      "backgroundColor": "#4267B2"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.justsellit.app",
      "config": {
        "googleSignIn": {
          "reservedClientId": "com.googleusercontent.apps.848015990033-72hkbv70h5lhrfjq6nae1oavnf4dq2m8"
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/appiconwithtitle.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.justsellit.app",
      "versionCode": 2
    },
    "web": {
      "favicon": "./src/assets/appicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ]
  }
}
